@model IEnumerable<Web.Models.User>

@{
    ViewBag.Title = "Climbers";
}

<h2>@ViewBag.Title</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            # Climbs
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Climbs.Count)
        </td>
    </tr>
}

</table>
